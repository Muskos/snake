{
  "name": "snake",
  "root": "packages/snake",
  "sourceRoot": "packages/snake/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "options": {
        "outputPath": "dist"
      }
    },
    "build:watch": {
      "executor": "@nx/vite:build",
      "options": {
        "outputPath": "dist",
        "watch": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/snake/src/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [],
      "options": {
        "passWithNoTests": true
      }
    }
  }
}